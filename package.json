{
  "name": "ui-starter",
  "version": "1.0.0",
  "main": "index.js",
  "author": "AJ Meyghani",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "npm-run-all -s --parallel dev:webpack dev:server",
    "predev:webpack": "rimraf dist",
    "dev:server": "nodemon server/server -w server/**/*.js",
    "dev:webpack": "webpack --config config/webpack.dev.js -w",
    "test": "jest --config config/jest.config.js  --no-cache",
    "dev:test": "jest --config config/jest.config.js  --no-cache --watch",
    "tdd": "yarn dev:test",
    "test:cover": "yarn test --coverage",
    "lint": "eslint config __mocks__ src server --fix",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "pretty": "prettier  --write \"{src,__mocks__,config,server}/**/*.js\"",
    "pl": "yarn pretty && yarn lint"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-react": "^7.13.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "1.17.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  }
}
